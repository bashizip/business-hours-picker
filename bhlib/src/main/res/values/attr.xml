        <attr name="dayOfWeek" format="string" />
        <attr name="isOpenDay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BusinessHoursWeekView">
        <attr name="bhIcon" format="integer" />
        <attr name="bhTextColor" format="color" />
